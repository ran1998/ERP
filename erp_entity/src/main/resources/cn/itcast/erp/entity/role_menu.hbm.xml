<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.itcast.erp.entity.Role_menu" table="role_menu" >
        <id name="uuid" >
        	<generator class="org.hibernate.id.SequenceGenerator"><param name="sequence">role_menu_seq</param></generator>
        </id>

        <property name="roleuuid" /> 
        <property name="menuuuid" /> 
 
    </class>
</hibernate-mapping>
